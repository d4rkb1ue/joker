<%- include header %>
    <script src="/js/jquery-ui.min.js"></script>
    <script>
        $(function(){
            $( "#tabs" ).tabs({
                load: function(event,ui){
                    $('#tabs li').removeClass('active')
                    ui.tab.addClass('active');
                    $('#project-panel-row>div.ui-tabs-panel>div.project-panel-panel').remove();
                    $('#project-panel-row').append(ui.panel.clone());
                }
            });
        });
    </script>

    <div class="container project-panel">
        <div id="project-panel-row" class="row">

            <div id="tabs" class="project-panel-menu col-md-3 col-lg-3 col-sm-3">
                <ul class="nav nav-pills nav-stacked">
                    <li role="presentation" class="active"><a href="/back-history">支持过的项目</a></li>
                    <li role="presentation"><a href="/created">创立的项目</a></li>

                </ul>
            </div>

        </div>
    </div>

    <%- include footer %>