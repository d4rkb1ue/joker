<div id="profile-panel" class="profile-panel col-xs-12 col-md-10 col-lg-10 col-sm-10">

  <h2 class="profile-title">支持过的项目</h2>
  <div class="table-responsive">
    <table class="table table-striped table-hover">
      <tr>
        <th>
          项目
        </th>
        <th style="width:240px">
          支持选项
        </th>
        <th style="width:70px">
          金额
        </th>
        <th>
          支付方式
        </th>
        <th>
          备注
        </th>
        <th>
          状态
        </th>
        <th>
          创建时间
        </th>
      </tr>
      <% orders.forEach(function(order,index){ %>
        <tr>
          <td>
            <a href="/project-funding/<%= order.proj_id %>">
                    <img src="/uploads/<%= order.feature_img %>" alt="<%= order.feature_img %>">
                    <b><%= order.proj_name %></b>
                  </a>
          </td>
          <td>
            <%= order.details || "" %>
          </td>
          <td>
            ¥
            <%= order.rw_amout %>
          </td>
          <td>
            <%= order.payment %>
          </td>
          <td>
            <%= order.note || '无' %>
          </td>
          <td>
            <%= order.status %>
          </td>
          <td>
            <%= new Date(order.history.created).toLocaleString().substring(0,10) %>
              <br/>
              <%= new Date(order.history.created).toLocaleString().substring(11) %>
          </td>
        </tr>
        <% }) %>

    </table>
  </div>

</div>